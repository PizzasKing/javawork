package objectarray.dogs;

public class DogArray {

	public static void main(String[] args) {
		Dog[] dogs= new Dog[4];
		dogs[0].setDog("백구", "진돗개");
		dogs[1].setDog("해피", "시츄");
		dogs[2].setDog("대박", "불독");
		dogs[3].setDog("사랑", "치와와");
		
		for(int i=0; i<dogs.length;i++) {
			System.out.println(dogs[i]);
		}
		

	
	}
}
