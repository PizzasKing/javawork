package inheritance.person;

public class Person {
	String name;
	int age;
	
	public Person(String name, int age) {
		this.name = name;
		this.age = age;
	}
	
	public Person(String name2, int age2) {
		// TODO Auto-generated constructor stub
	}

	public String getName() {
		return name;
	}
	public int getAge() {
		return age;
	}
}
