		shop1.김치찌개();
		shop1.된장찌개();
		shop1.비빔밥();
		System.out.println("=====================");
		HeadShop headshop = new HeadShop();
		headshop.김치찌개();
		headshop.된장찌개();
		headshop.비빔밥();
		
		System.out.println("=====================");
		Shop2 shop2 = new Shop2();
		shop2.김치찌개();
		shop2.된장찌개();
		shop2.비빔밥();
		
		

	}

}
