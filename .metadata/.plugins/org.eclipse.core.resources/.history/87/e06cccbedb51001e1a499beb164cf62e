package overriding.shop;

public class HeadShop {
	
	public HeadShop() {
		System.out.println("본점입니다.");
	}
	
	public void 된장찌개() {
		System.out.println("된장찌개: 6,000원");
	}
	public void 김치찌개() {
		System.out.println("김치찌개: 7,000원");
		}
	public void 비빔밥() {
		System.out.println("비빔밥: 8,000원");
	}
}
